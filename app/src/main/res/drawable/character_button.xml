<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item>
        <shape android:shape="rectangle">
            <solid android:color="@android:color/transparent"/>
            <stroke android:width="8dp" android:color="#3D7CE6"/>
            <size android:width="140dp" android:height="45dp"/>
            <corners
                android:bottomRightRadius="25dp"
                android:bottomLeftRadius="25dp"
                android:topLeftRadius="25dp"
                android:topRightRadius="25dp"/>




        </shape>
    </item>


    <item>
        <shape>
            <padding
                android:bottom="2dp"
                android:top="-1dp"
                />

            <solid android:color="#0FBE1010" />

            <corners android:radius="30dp" />
        </shape>
    </item>


    <item>
        <shape>
            <padding
                android:bottom="7dp"
                android:left="7dp"
                android:right="7dp"
                android:top="7dp" />

            <solid android:color="#1E3554" />

            <corners android:radius="30dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding
                android:bottom="10dp"
                android:left="10dp"
                android:right="10dp"
                android:top="10dp" />
            <solid android:color="#000000" />
            <corners android:radius="30dp" />
        </shape>
    </item>



</layer-list>